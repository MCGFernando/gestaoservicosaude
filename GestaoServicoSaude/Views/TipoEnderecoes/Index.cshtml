@model IEnumerable<GestaoServicoSaude.Models.ViewModel.TipoEnderecoViewModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TipoEndereco.Tipo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoEndereco.Descricao)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                    @Html.DisplayFor(modelItem => item.TipoEndereco.Tipo)
            </td>
            <td>
                    @Html.DisplayFor(modelItem => item.TipoEndereco.Descricao)
            </td>
            <td>
                    <a asp-action="Edit" asp-route-id="@item.TipoEndereco.Id">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.TipoEndereco.Id">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.TipoEndereco.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
<partial name="_ModalPartial" model="@Model" />


